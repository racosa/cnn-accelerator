puts {-- Note: Precision Synthesis Started}

proc get_state { args } {
   set state {}
   catch {
      set impl [get_impl_property -name]
      set psi [open $impl/${impl}.psi r]
      while {[gets $psi line] >= 0} {
            if {[regexp -- "PROP key='statename' .*value='(.+)'" $line du state]} {
                  break
            }
      }
      close $psi
   }
   set state
}
proc run_setup { args } {
   ## Setup Project
   new_project -name psr_concat_vhdl -folder . -createimpl_name psr_concat_vhdl_impl -force
   set_project_property -usetempdir false
   set_input_dir .
   setup_design -var "analyze_extra_options=-override -keeplast"

   ## Add source HDL files
   set CATAPULT_HOME /softl3/catapultc10_0a/64bit/Mgc_home
   add_input_file -work work ./concat_rtl.vhdl -format vhdl
#   add_input_file -work work ./concat_rtl.vhdl -format vhdl
   setup_design -design=apply_conv

   ## Setup global frequence
   setup_design -frequency 100.0

   ## Setup technology settings
   setup_design -manufacturer Xilinx -family ARTIX-7 -part 7A100TCSG324 -speed -2
   setup_design -variable bumpup_device=true
   setup_design -addio=true
   setup_design -edif=true
   setup_design -retiming=false

if {[string compare [lindex [split [get_version] .] 0] "2010a"] >= 0} {
setup_place_and_route -flow "ISE" -command "Integrated Place and Route" -ba_format VHDL
}

   ## Add timing constraint file
   add_input_file ./concat_rtl.vhdl.psr.sdc -format SDC

   ## Disable auto relaxation
   if {! [catch {setup_place_and_route -command {Generate Vendor Constraint File} -enable_auto_offset_relaxation 0}]} {
      puts "Info: Disabling Auto Offset Relaxation in Vendor Constraint file."
   }
   save_project
}

